{
    "name": "datan",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test.e2e": "cypress run",
        "test.e2e:chrome": "cypress run --browser chrome",
        "test.e2e:firefox": "cypress run --browser firefox",
        "test.e2e:headed": "cypress run --headed",
        "test.unit": "vitest",
        "prettier": "npx prettier . --write",
        "lint": "eslint",
        "format": "npm run prettier && npm run lint",
        "sync": "ionic cap sync",
        "sync-android": "ionic cap sync android",
        "sync-ios": "ionic cap sync ios",
        "android-live-reload": "ionic cap run android -l --external",
        "ios-live-reload": "ionic cap run ios -l --external",
        "open-android": "ionic cap open android",
        "open-ios": "ionic cap open ios",
        "add-ios": "ionic cap add ios",
        "add-android": "ionic cap add android"
    },
    "dependencies": {
        "@capacitor/android": "7.4.1",
        "@capacitor/app": "7.0.1",
        "@capacitor/core": "7.4.1",
        "@capacitor/haptics": "7.0.1",
        "@capacitor/ios": "7.4.1",
        "@capacitor/keyboard": "7.0.1",
        "@capacitor/splash-screen": "^7.0.1",
        "@capacitor/status-bar": "7.0.1",
        "@ionic/react": "^8.7.1",
        "@ionic/react-router": "^8.7.1",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "ionicons": "^7.4.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4"
    },
    "devDependencies": {
        "@capacitor/cli": "7.4.1",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^16.2.0",
        "@testing-library/user-event": "^14.4.3",
        "@trivago/prettier-plugin-sort-imports": "^5.2.2",
        "@types/react": "19.0.10",
        "@types/react-dom": "19.0.4",
        "@vitejs/plugin-legacy": "^5.0.0",
        "@vitejs/plugin-react": "^4.7.0",
        "cypress": "^13.17.0",
        "eslint": "^9.32.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.1.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^15.15.0",
        "jsdom": "^22.1.0",
        "prettier": "3.6.2",
        "sass": "^1.89.2",
        "terser": "^5.4.0",
        "typescript": "^5.8.3",
        "typescript-eslint": "^8.38.0",
        "vite": "~5.2.0",
        "vitest": "^0.34.6"
